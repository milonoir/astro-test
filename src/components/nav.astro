---
const { items } = Astro.props;
---

<nav aria-label="Main Navigation" class="items-center w-full md:w-auto hidden md:flex md:mx-5 text-default overflow-y-auto overflow-x-hidden md:overflow-y-visible md:overflow-x-auto md:justify-self-center">
    <ul>
        {items.map(({ text, href }) => (
            <li>
                <a href={href}>{text}</a>
            </li>
        ))}
    </ul>
</nav>
